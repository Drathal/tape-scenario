    "type": "git",
    "url": "git+https://github.com/ksmithbaylor/tape-scenario.git"
  },
  "keywords": [
    "tape",
    "scenario",
    "outline",
    "cucumber",
    "tests",
    "testing"
  ],
  "author": "Kevin Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithbaylor/tape-scenario/issues"
  },
  "homepage": "https://github.com/ksmithbaylor/tape-scenario#readme",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "tap-difflet": "^0.4.0",
    "tape": "^4.5.1"
  }
